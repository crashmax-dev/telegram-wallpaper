{
  "name": "twallpaper",
  "version": "1.2.0",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.es.js",
  "main": "./dist/index.cjs.js",
  "main:umd": "./dist/index.umd.js",
  "files": [
    "dist/index.cjs.js",
